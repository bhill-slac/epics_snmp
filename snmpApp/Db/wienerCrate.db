record(longin, "$(SYS):$(SUBSYS):$(CRATE):SupplyStatus")
{
    field(DESC, "Supply off/on status")
    field(SCAN, "2 second")
    field(DTYP, "Snmp")
    field(INP,  "@$(CRATE) public %(W)sysMainSwitch.0 ( 100 i")
    field(LOPR, "0")
    field(HOPR, "1")
}

